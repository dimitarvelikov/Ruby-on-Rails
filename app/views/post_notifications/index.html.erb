<p id="notice"><%= notice %></p>

<h1>Post Notifications</h1>

<table>
  <thead>
    <tr>
      <th>Post</th>
      <th>Seen</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @post_notifications.each do |post_notification| %>
      <tr>
        <td><%= post_notification.post_id %></td>
        <td><%= post_notification.seen %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

